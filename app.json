{
  "expo": {
    "name": "PaperFree",
    "slug": "paperfree",
    "version": "1.0.0",
    "orientation": "landscape",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ajas.paperfree"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.ajas.paperfree"
    },
    "web": {
      "bundler": "webpack"
    },
    "plugins": [
      "expo-screen-orientation"
    ]
  }
}
